@model IEnumerable<Testlogin.ServiceReference1.RegisterViewModel>

@{
    /**/

    ViewBag.Title = "Account";
}

<h2>Account</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
@foreach (var item in ViewBag.data)
{
    <div class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-sm-2" for="User">Name :</label>
            <div class="col-md-10">
                <label class="control-label col-sm-2" for="User">@item.Name</label>
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-sm-2" for="User">Lastname :</label>
            <div class="col-md-10">
                <label class="control-label col-sm-2" for="User">@item.Lastname</label>
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-sm-2" for="User">Email :</label>
            <div class="col-md-10">
                <label class="control-label col-sm-2" for="User">@item.email</label>
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-sm-2" for="User">Age :</label>
            <div class="col-md-10">
                <label class="control-label col-sm-2" for="User">@item.Age</label>
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="button" class="btn btn-info" value="Edit account" onclick="location.href='@Url.Action("Update", "Home")'" />
                <input type="button" class="btn btn-info" value="Delete account" onclick="location.href='@Url.Action("Deleteid", "Home")'" />
                <input type="button" class="btn btn-info" value="Change password" onclick="location.href='@Url.Action("Repassword", "Repassword")'" />
            </div>
        </div>
    </div>

 }


